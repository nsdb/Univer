<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nsdb.univer.ui"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyTheme"
        >

        
        <activity android:name="LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="RegisterUser">
            <intent-filter>
                <action android:name="RegisterUser" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name="RegisterBook">
            <intent-filter>
                <action android:name="RegisterBook" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name="RangeSettingA">
            <intent-filter>
                <action android:name="RangeSetting" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name="TabMain2">
            <intent-filter>
                <action android:name="TabMain" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name="BookDetail">
            <intent-filter>
                <action android:name="BookDetail" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity android:name="ProfessorDetail">
            <intent-filter>
                <action android:name="ProfessorDetail" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
    </application>

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>